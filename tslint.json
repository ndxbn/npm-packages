{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "cyclomatic-complexity": true,
    "no-any": true,
    "no-bitwise": false,
    "no-parameter-properties": true,
    "no-switch-case-fall-through": true,
    "object-literal-sort-keys": false
  }
}
